test5    start   1000                                            
first    j       begin                                           
five     word    5                                               
xxx      resw    1                                               
begin    lda     zero                                            
         ldx     zero                                            
loop     jsub    getc                                            
         mul     shift                                           
         sta     atemp                                           
         jsub    getc                                            
         add     atemp                                           
         stch    exaddr,x                                        
         tix     five                                            
         j       loop                                            
getc     stx     temp                                            
         ldx     incnt                                           
         lda     incnt                                           
         add     one                                             
         sta     incnt                                           
         ldch    indev,x                                         
         comp    eof                                             
         jeq     exaddr                                          
         comp    asciz                                           
         jlt     getc                                            
         sub     asciz                                           
         comp    ten                                             
         jlt     return                                          
         sub     seven                                           
return   ldx     temp                                            
         rsub                                                    
zero     word    0                                               
eof      word    63                                              
temp     resw    1                                               
incnt    word    0                                               
indev    byte    c'0010030C1006'                                 
         byte    c'000005FFFFFF?'                                
exaddr   resb    12                                              
shift    word    16                                              
atemp    resw    1                                               
one      word    1                                               
asciz    word    48                                              
ten      word    10                                              
seven    word    7                                               
         end     first                                           
